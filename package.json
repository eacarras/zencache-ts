{
  "name": "zencache-ts",
  "version": "0.4.0",
  "description": "In-memory caching service in TypeScript with LRU+TTL and optional TinyLFU, HTTP + RESP adapters, Prometheus metrics.",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "dev:resp": "PORT=8080 RESP_PORT=6380 tsx watch src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "bench": "tsx scripts/bench.ts"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}